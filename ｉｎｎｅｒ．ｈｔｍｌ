<script type="text/javascript">
  var title=document.title;
  var title1='[星期一]谷粒课堂'
    var title2='[新提醒]谷粒课堂'
    var index=0;
  setInterval(function(){
      index++;
      if(index%2){
          document.title=title1;
      }else{
          document.title=title2;
      }
  },500)
</script>
